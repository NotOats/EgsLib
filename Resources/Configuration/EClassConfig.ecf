  MapIcon: map_ply

  /* Misc stuff */
  IsEnemy: false
  /* ItemsOnEnterGame: "Drill,Pistol" */
  DropInventoryEntity: Backpack
  UpdateClusterInfo: true   /* If this entity can move on cluster other than terrain */
}

