  Next_1: Eden_DataResearcher_NoChipsPristine
  NextIf_1: "!HasItem('Eden_DataChipT3', 1)"
  Next_2: Eden_DataResearcher_TurninAskPristine
  NextIf_2: "HasItem('Eden_DataChipT3', 1)"
}

{ +Dialogue Name: Eden_DataResearcher_TurninPristine
  Output: "@d0Thank you for your contribution.\nWe have currently decrypted {Eden_UCHResearch} Terabytes of the Progenitor data. \n100 Terabytes are needed."
  Next_1: Eden_DataResearcher_NoChipsPristine
  NextIf_1: "!HasItem('Eden_DataChipT3', 1)"
  Next_2: Eden_DataResearcher_TurninAskPristine
  NextIf_2: "HasItem('Eden_DataChipT3', 1)"
}

{ +Dialogue Name: Eden_DataResearcher_NoChipsPristine
  Output: "@d0It seems you do not have any <color=#69F5CD>Pristine Data Chips</color>. You can find them by hacking into data caches found in some areas of space."
  Option_1: "I want to ask something else." 
  OptionNext_1: Eden_DataResearcher_Start
}

{ +Dialogue Name: Eden_DataResearcher_TurninAskPristine
  Output: "@d0How many would you like to turn in?"

  Option_1: "1 Pristine Data Chip <color=#00ff00>(+100,000 credits)</color>" 
  OptionNext_1: Eden_DataResearcher_TurninPristine
  OptionIf_1: "HasItem('Eden_DataChipT3', 1)"
  OptionExecute_1: "RemoveItem('Eden_DataChipT3', 1); Player.Credit += 100000; Eden_UCHResearch = Eden_UCHResearch + 10; Player.UpgradePoints += 100"

  Option_2: "5 Pristine Data Chips <color=#00ff00>(+500,000 credits)</color>" 
  OptionNext_2: Eden_DataResearcher_TurninPristine
  OptionIf_2: "HasItem('Eden_DataChipT3', 5)"
  OptionExecute_2: "RemoveItem('Eden_DataChipT3', 5); Player.Credit += 500000; Eden_UCHResearch = Eden_UCHResearch + 50; Player.UpgradePoints += 500"
  
  Option_3: "10 Pristine Data Chips <color=#00ff00>(+1,000,000 credits)</color>" 
  OptionNext_3: Eden_DataResearcher_TurninPristine
  OptionIf_3: "HasItem('Eden_DataChipT3', 10)"
  OptionExecute_3: "RemoveItem('Eden_DataChipT3', 10);  Player.Credit += 1000000; Eden_UCHResearch = Eden_UCHResearch + 100; Player.UpgradePoints += 1000"
  
  Option_4: "25 Pristine Data Chips <color=#00ff00>(+2,500,000 credits)</color>" 
  OptionNext_4: Eden_DataResearcher_TurninPristine
  OptionIf_4: "HasItem('Eden_DataChipT3', 25)"
  OptionExecute_4: "RemoveItem('Eden_DataChipT3', 25);  Player.Credit += 2500000; Eden_UCHResearch = Eden_UCHResearch + 250; Player.UpgradePoints += 2500"
}

{ +Dialogue Name: Eden_DataResearcher_ResearchComplete
  Output: "Do you want to use up 100 Terabytes of research to create a <color=#ff8000>Progenitor Translation Key</color>?"
  Option_1: "Yes." 
  OptionNext_1: Eden_DataResearcher_ResearchComplete2
  OptionExecute_1: "Eden_UCHResearch = Eden_UCHResearch - 100; AddItem('Token', 1, 6104)"
  OptionIf_1: "Eden_UCHResearch >= 100"
  Option_2: "Not at this time. I want to ask something else." 
  OptionNext_2: Eden_DataResearcher_Start
  Option_3: "Goodbye." 
  OptionNext_3: End
}

{ +Dialogue Name: Eden_DataResearcher_ResearchComplete2
  Output: "Using the information decrypted from the Data Chips you have turned in, we were able to replicate this translation key. This should grant you access to Progenitor computer systems. If you need another key, turn in more Data Chips."
  Option_1: "Thank you. I want to ask something else." 
  OptionNext_1: Eden_DataResearcher_Start
  Option_2: "Goodbye." 
  OptionNext_2: End
}

{ +Dialogue Name: Eden_DataResearcher_WarpUpgrade
  # Output: "@d0If you have any navigational data I can use it to upgrade your capital vessel maximum warp drive range. Max of 25 light year increase. \nYou currently have a {Player.Skills['Eden_WarpRange']} increased range to your warp drive."
  Output: "@d0Navigational data can be used to upgrade your capital vessel maximum warp drive range. I no longer accept navigational data. Now that is handled by the Research Computers on UCH Research Vessels."
  Option_1: "I want to ask something else."
  OptionNext_1: Eden_DataResearcher_Start
  Option_2: "Goodbye."
  OptionNext_2: End
}


