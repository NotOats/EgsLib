  #: "<nl>"
  Functions: "void FishChanceResult() {<nl> if ( QuestFishChance == 0 ) {<nl>  HowManyFish = 1;<nl>  AddItem('HWSFish', HowManyFish);<nl>  QuestFishChance++;<nl>  FishingResult = LocF('eden_dialogue_m84uOqG', HowManyFish);<nl> } else if ( QuestFishChance < 16 && Random(0,5) == 0 ) {<nl>  HowManyFish = 1;<nl>  AddItem('HWSFish', HowManyFish);<nl>  QuestFishChance++;<nl>  FishingResult = LocF('eden_dialogue_m84uOqG', HowManyFish);<nl> } else if ( QuestFishChance >= 20 && QuestFishChance < 25 && Random(0,4) == 0 ) {<nl>  HowManyFish = 1;<nl>  AddItem('HWSFish', HowManyFish);<nl>  QuestFishChance++;<nl>  FishingResult = LocF('eden_dialogue_m84uOqG', HowManyFish);<nl> } else if ( QuestFishChance >= 25 && QuestFishChance < 33 && Random(0,3) == 0 ) {<nl>  HowManyFish = 1;<nl>  AddItem('HWSFish', HowManyFish);<nl>  QuestFishChance++;<nl>  FishingResult = LocF('eden_dialogue_m84uOqG', HowManyFish);<nl> } else if ( QuestFishChance >= 33 && QuestFishChance < 50 && Random(0,2) == 0 ) {<nl>  HowManyFish = 2;<nl>  AddItem('HWSFish', HowManyFish);<nl>  QuestFishChance++;<nl>  FishingResult = LocF('eden_dialogue_m84uOqG', HowManyFish);<nl> } else  if ( QuestFishChance >= 50 && QuestFishChance < 100 && Random(0,1) == 0 ) {<nl>  HowManyFish = 2;<nl>  AddItem('HWSFish', HowManyFish);<nl>  QuestFishChance++;<nl>  FishingResult = LocF('eden_dialogue_m84uOqG', HowManyFish);<nl> } else {<nl>  FishingResult = LocF('eden_dialogue_OKKudKS', QuestFishChance);<nl> }<nl><nl> if ( QuestFishChance >= 100 ) {<nl>  HowManyFish = 3;<nl>  AddItem('HWSFish', HowManyFish);<nl>  QuestFishChance++;<nl>  FishingResult = LocF('eden_dialogue_m84uOqG', HowManyFish);<nl> }<nl>}"
